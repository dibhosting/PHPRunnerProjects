<?xml version="1.0"?>
<root>
	<brick>
		<name>jobs_gridsearch</name>
		<orientation></orientation>
		<file>jobs_gridsearch</file>
		<plexpr></plexpr>
		<xtname></xtname>
	</brick>
	<brick>
		<name>view_header</name>
		<orientation></orientation>
		<table>jobsjobs</table>
		<file>jobs_viewfieldheader</file>
		<plexpr></plexpr>
		<xtname></xtname>
	</brick>
	<brick>
		<name>search</name>
		<orientation></orientation>
		<file>jobs_search_horizontal</file>
		<plexpr>@TABLE.arrFieldObj[bAdvancedSearch].len || @TABLE.strDataSourceTable="admin_admembers"</plexpr>
		<xtname>searchform_block</xtname>
		<requiresTable>true</requiresTable>
		<table>jobsjobs</table>
		<page>list</page>
	</brick>
	<brick>
		<name>search_buttons</name>
		<orientation>h</orientation>
		<file>jobs_search_buttons_horizontal</file>
		<plexpr>!@TABLE.bFlexibleSearch &&  Fields[bSearch=1].len || @TABLE.bFlexibleSearch && Fields[bAdvancedSearch=1 || bSearch=1].len</plexpr>
		<xtname>searchformbuttons_block</xtname>
		<table>jobsjobs</table>
		<page>list</page>
	</brick>
	
	<brick>
		<name>grid</name>
		<orientation></orientation>
		<version>2</version>
		<file>newgrid</file>
		<plexpr></plexpr>
		<table>jobsjobs</table>
		<page>list</page>
		<xtname>grid_block</xtname>
		<wordWrap>true</wordWrap>
		<parameters>
			<parameter>
				<name>columns</name>
				<default>newgrid_vertical5.htm</default>
			</parameter>
			<parameter>
				<name>hgrid</name>
				<default>newgrid_horizontal.htm</default>
			</parameter>
			<parameter>
				<name>gridheader_horizontal</name>
				<default>newgridheader_h.htm</default>
			</parameter>
			<parameter>
				<name>gridfooter_horizontal</name>
				<default>newgridfooter_h.htm</default>
			</parameter>
			<parameter>
				<name>gridrow_horizontal</name>
				<default>newgridrow_h.htm</default>
			</parameter>
			<parameter>
				<name>gridfieldsheader_horizontal</name>
				<default>jobs_gridfieldsheader_h.htm</default>
			</parameter>
			<parameter>
				<name>gridcolumnheader_horizontal</name>
				<default>jobs_gridcolumnheader_h.htm</default>
			</parameter>
			<parameter>
				<name>gridfieldsfooter_horizontal</name>
				<default>jobs_gridfieldsfooter_h.htm</default>
			</parameter>
			<parameter>
				<name>gridcolumnfooter_horizontal</name>
				<default>jobs_gridcolumnfooter_h.htm</default>
			</parameter>
			<parameter>
				<name>gridfieldsfooter2_horizontal</name>
				<default>jobs_gridfieldsfooter2_h.htm</default>
			</parameter>
			<parameter>
				<name>gridfieldsrow_horizontal</name>
				<default>jobs_gridfieldsrow_h.htm</default>
			</parameter>
			<parameter>
				<name>gridcolumnrow_horizontal</name>
				<default>jobs_gridcolumnrow_h.htm</default>
			</parameter>
			<parameter>
				<name>vgrid</name>
				<default>newgrid_vertical.htm</default>
			</parameter>
			<parameter>
				<name>gridfieldsrow_vertical</name>
				<default>newgridfieldsrow_v.htm</default>
			</parameter>
			<parameter>
				<name>gridcolumnrow_vertical</name>
				<default>newgridcolumnrow_v.htm</default>
			</parameter>
			<parameter>
				<name>grid_horizontal</name>
				<default>jobs_grid_horizontal.htm</default>
			</parameter>
		</parameters>
	</brick>
</root>